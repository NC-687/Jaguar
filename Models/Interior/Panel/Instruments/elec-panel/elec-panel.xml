<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<path>elec-panel.ac</path>
	
	<animation>
		<type>scale</type>
        <object-name>Back</object-name>
		<object-name>BATT</object-name>
		<object-name>TR-1</object-name>
		<object-name>TR-2</object-name>
		<object-name>ALT-1</object-name>
		<object-name>ALT-2</object-name>
		<object-name>Pitot-Heat</object-name>
		<object-name>Mount</object-name>
        <object-name>NVG</object-name>
		<x-offset>0.8</x-offset>
		<y-offset>0.8</y-offset>
		<z-offset>0.8</z-offset>
	</animation>
	
	<animation>
		<object-name>BATT</object-name>
		<type>knob</type>
		<property>/controls/electrical/battery-switch</property>
		<factor>-60</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>-0.03345</x-m>
			<y-m> 0.03980</y-m>
			<z-m> 0.07928</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/battery-switch</property>
                <min>-1</min>
                <max>0</max>
            </binding>
        </action>
	</animation>
	
	<animation>
		<object-name>TR-1</object-name>
		<type>knob</type>
		<property>/controls/electrical/tru-1-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.00037</x-m>
			<y-m>0.03972</y-m>
			<z-m>0.07938</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-1-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-1-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
	
	<animation>
		<object-name>TR-2</object-name>
		<type>knob</type>
		<property>/controls/electrical/tru-2-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.02474</x-m>
			<y-m>0.03980</y-m>
			<z-m>0.07928</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-2-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/tru-2-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
	
	<animation>
		<object-name>ALT-1</object-name>
		<type>knob</type>
		<property>/controls/electrical/alternator-1-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.06499</x-m>
			<y-m>0.03980</y-m>
			<z-m>0.07922</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-1-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-1-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
	
	<animation>
		<object-name>ALT-2</object-name>
		<type>knob</type>
		<property>/controls/electrical/alternator-2-switch</property>
		<factor>-30</factor>
		<drag-scale-px>5</drag-scale-px>
		<center>
			<x-m>0.08971</x-m>
			<y-m>0.03980</y-m>
			<z-m>0.07928</z-m>
		</center>
		<axis>
			<y>1</y>
		</axis>
		<action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-2-switch</property>
                <min>-1</min>
                <max>1</max>
            </binding>
        </action>
		<release>
            <binding>
                <command>property-adjust</command>
                <property>/controls/electrical/alternator-2-switch</property>
                <step>0</step>
                <min>-1</min>
                <max>0</max>
            </binding>
        </release>
	</animation>
</PropertyList>

