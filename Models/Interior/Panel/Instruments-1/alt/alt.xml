  <animation>
    <type>pick</type>
    <object-name>inhg.dn</object-name>
    <visible>0</visible>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/altimeter/setting-inhg</property>
        <step>-0.01</step>
        <min>26.90</min>
        <max>32.10</max>
        <wrap>0</wrap>
